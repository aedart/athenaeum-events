{
    "name": "aedart/athenaeum-events",
    "description": "Offers a way to register event listeners and subscribers via configuration",
    "keywords": [
        "Athenaeum",
        "Events",
        "Listeners",
        "Subscribers"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/events": "^8.15",
        "aedart/athenaeum-support": "^5.10"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Events\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.10.x-dev"
        }
    }
}
